/** gou.styl # */
@import "nib";
@import "mixins";
@import "util";

html{height:100%;}
body{height:100%; font-size:px(12);/*12 14 16 20*/ color:#656565;}
a{color:#666;} a:active{color:#e05a3f;}
img[src$="blank.gif"], img[src$="blank-img.gif"]{height:100% !important; background:#fff url("../img/pic_imgDefault.png") no-repeat center; background-size:px(38);}
.module{min-height:100%; background:#fafafa; overflow:hidden; overflow-x: auto;}
#home .module{background:#ededed;}
.err-border {border: 1px solid #f63 !important;}

// 页面框架或公用模块
.hd{
	min-width: px(320);
	.logo, .title, nav{
		width:px(320); 
		margin:0 auto;
	}
	.top-wrap{
		background-color:#e05a3f; 
		border-top:solid 1px #e7755b; 
		border-bottom:solid 1px #b34832; 
		h1{
			color:#fff;
		} 
		a{
			display:block;
		}
		.logo{
			display:-webkit-box; 
			overflow:hidden;
			h1{
				width:px(114); 
				padding-top:px(7); 
				text-align:center; 
				font-size:px(20);
				background:url("../img/amigo_subtitle.jpg") no-repeat px(65) px(29); 
				background-size:px(32);
			}
			a{
				-webkit-box-flex:1; 
				position:relative; 
				margin:px(5) px(5) px(5) 0; 
				line-height:px(32); 
				background:#fff; 
				border-radius:5px;
				em{
					display:block; 
					color:#aaa; 
					font-size:px(14); 
					text-indent:px(8);
				}
				i{
					display:block; 
					position:absolute; 
					top:px(5); 
					right:px(7); 
					width:px(22); 
					height:px(22);
					background:url("../img/ico_search.jpg") no-repeat center; 
					background-size:contain;
				}
			}
		}
		.title{
			position:relative; overflow:hidden;
			h1{font-size:px(16); margin-top:px(14); margin-bottom:px(14);}
			a{
				float:left; width:px(32); line-height:px(32); text-align:center; margin:px(5);
				&:before{content:""; display:inline-block; vertical-align:middle; width:px(12); height:px(12); margin-left:px(6);
					border:solid #fff; border-width:2px 0 0 2px; transform:rotate(-45deg);}
				&:active{background:#bf4d36;}
			}
		}
	}
	wrapheader nav, .main-nav{
		ul{display:flex;}
		li{
			flex:1; width:33.3%; line-height:px(31); background:#545454; border-bottom:solid 2px #545454; font-size:px(14);
			span, a{display:block; height:px(29); text-align:center; border-left:solid 1px #585858; border-right:solid 1px #4b4b4b;}
			a{color:#f2f2f2;}
			&:first-child > *{border-left:0;} &:last-child > *{border-right:0;}
			&.selected{
				background:#4a4a4a; border-bottom-color:#e05a3f;
				span{color:#c4c4c4;}
			}
		}
	}
	wrapsubnav nav, wrapbrandnav nav, .sub-nav{
		width:px(320); margin:0 auto;
		ul{display:flex; width:100%;}
		li{
			flex:1; line-height:px(30); padding:0 px(6); text-align:center; background:#5d5d5d;
			span, a{display:block; margin-bottom:1px; color:#f2f2f2;}
			&.selected span, &.selected a{border-bottom:solid 2px #dfdfdf;}
		}
	}
	wrapsubnav nav {
		ul {
			border-bottom: 1px solid #ddd;
		}
		li {
			line-height: px(35);
			background: #fbfbfb;
			padding: 0 px(20);
			span, a {
				color: #666;
				font-size: px(13);
			}
			&.selected span, &.selected a {
				color: #ff6633;
				border-bottom: solid 2px #ff6633;
			}
			.ico-put {
				margin-left: px(5);
			}
		}
		&.filter {
			ul {
				border-bottom: 1px solid #f5f5f5;
			}
			li {
				background: #fafafa;
				padding: 0 px1080(30);
				overflow: hidden;
				span {
					color: #888;
					font-size: px1080(42)
				}
				.txt {
					float: left;
				}
				.filter-btn {
					float: right;
				}
				.ico-put {
					position: relative;
					top: px1080(-5);
				}
			}
		}
	}
	wrapsubnav .filter-wrap {
		width: px(320);
		margin: 0 auto;
	}
}

.ft{
	padding:0 0 px(26); width:px(320); margin:0 auto; text-align:center;
	&.appft{padding-bottom:0;}
	.help{
		padding:px(8) 0; background:#fff; line-height:1.8;
		span{display:block;}
		a{display:block; color:#938c8a; text-decoration:underline;}
	}
	.copyright p{margin-top:px(10); color:#676767;}
}

.loading{
	height:px(32); color:#435b72; display:none; -webkit-box-align:center; -webkit-box-pack:center;
	.icon-label{display:block;}
	.icon-img{
		display:block; width:px(49); height:px(32); margin: 0 auto;
		background:url("../img/loading2.gif") no-repeat center; background-size:px(49) auto;
	}
}

.gotop{
	position:fixed; right:px(10); bottom:px(20); z-index:1000; opacity: 0;
	span{
		display:block; width:px(40); height:px(40); font-size:0; text-indent:-999px; color:transparent;
	}
	&.gotop-custom {
		bottom: px(65);
	}
}

@media screen and (max-device-width: 320px) {
	.gotop {
		span {
			background: url(../img/gotop_540.png) no-repeat center;
			&:active {
				background: url(../img/gotop_active_540.png) no-repeat center;
			}
		}
	}
}
@media screen and (min-device-width: 320px) {
	.gotop {
		span {
			background: url(../img/gotop_1080.png) no-repeat center;
			&:active {
				background: url(../img/gotop_active_1080.png) no-repeat center;
			}
		}
	}
}
.gotop span, .gotop span:active {
	-webkit-background-size: contain;
	background-size: contain;
}

.msg-tip{
	position:fixed; left:0; bottom:px(10); width:100%; height:px(30); z-index:12; text-align:center; -webkit-transform: translateZ(0); transform: translateZ(0);
	span{display:inline-block; line-height:px(30); padding:0 px(20); background:rgba(0, 0, 0, .5); color:#fff; font-size:px(12);}
}

figure.item-pictext{
	display:-webkit-box; background:#fff;
	.pic{
		width:px(106);
		span{display:table-cell; vertical-align:middle; text-align:center; width:px(106); height:px(106);}
		img{max-width:80%; max-height:80%;}
	}
	.desc{
		-webkit-box-flex:1; overflow:hidden;
		h3{font-size:px(13); padding-top:px(12); color:#282828;}
		.price{font-size:px(13); margin-top:px(10); color:#b12828;}
		.text{font-size:px(10); margin-top:px(18); line-height:1.5;}
	}
	.options{width:px(30); text-align:center;}
}

.web-btn{
	text-align:center;
	a, button{
		display:inline-block; 
		font-size:px(16); 
		background:#ff9000; 
		color:#fff; 
		border-radius:5px; 
		line-height:px(38); 
		width:100%;
		border: none;		
	}
	.dis {
		background: #ccc;
		color: #fff;
	}
	.gray{
		background:#ededed; 
		color:#afafaf;
	}
}

//log in and register
.rount-rect{
	font-size:px(16); border:solid 1px; border-radius:5px;
	&.orange{color:#fff; border-color:#ed8b22; background-image:linear-gradient(top, #fd9e37, #ff7c00);}
	&.gray{color:#6e6f70; border-color:#e7e9eb; background-image:linear-gradient(top, #f1f2f5, #dee0e3);}
}
.arrow-rect{
	position:relative; border:solid 1px #c9c9c9; padding-left:px(10); padding-right:px(10); border-radius:5px;
	&:after{content:""; width:px(8); height:px(8); position:absolute; right:px(10); border:solid #757575; border-width:2px 2px 0 0;}
	&.right:after{transform:rotate(45deg); top:42%;}
	&.up:after{transform:rotate(-45deg); top:46%;}
	&.down:after{transform:rotate(135deg); top:38%;}
}
.btn-edit{background:url("../img/ico_edit.png") no-repeat center; background-size:px(28) px(26); display:inline-block; width:px(28); height:px(26);}
.text-show{
	padding:px(12) px(22); position:relative; border:dashed 1px #e1e1e1; font-size:px(14); border-radius:5px;
	.edit{position:absolute; right:px(22); top:45%;}
	li{margin-bottom:px(22); padding-left:px(64);} li:last-child{margin-bottom:0;}
	li em, li label{display:inline-block; margin-left:px(-68); width:px(64);}
}
.form-box{
	padding:px(12) px(22); font-size:px(14);
	.submit{text-align:center; margin-top:px(54); button{display:inline-block; width:px(182); padding:px(12) 0; text-align:center;}}
	li{margin-bottom:px(10); padding-left:px(64); overflow:hidden; line-height:1.8;} li:last-child{margin-bottom:0;}
	li em, li label{float:left; margin-left:px(-64); width:px(64); text-align:right;}
	&.long-form-box{
		li{padding-left:px(80);}
		li em, li label{margin-left:px(-80); width:px(80);}
		.select:after{right:px(20);}
	}
	li{
		input[type="text"], input[type="email"], input[type="tel"], input[type="number"], input[type="search"], select{
			border:solid 1px #bdbdbd; -webkit-appearance:none; background:#fff; border-radius:5px;
		}
		input[type="text"], input[type="email"], input[type="tel"], input[type="number"], input[type="search"]{
			padding:px(6) px(10); width:px(156); color:#515151;
		}
		.select{
			display:block; position:relative; font-size:px(14);
			&:after{content:""; display:block; position:absolute; right:px(30); top:40%; width:px(7); height:px(7);
				border:solid #7e7e7e; border-width:2px 0 0 2px; transform:rotate(225deg);}
			select{width:px(176); padding:px(6); color:#7f7f7f;}
		}
		
	}
}

#iScroll .popWrap{
	width:140px; background:rgba(0,0,0,0.6); color:#fff; text-align:center; font-size:14px; padding:10px;
	text-shadow:1px 1px 2px #000; border-radius:2px; position:absolute; z-index:9999; overflow:hidden;
}
#iScroll .hide {display:none; -webkit-box-shadow:none;}
#iScroll .show {display:block; -webkit-box-shadow:0 0 2px 2px rgba(0, 0, 0, 0.3);}

.JS-dbMask{background-color:#000; width:100%; height:100%; position:absolute; left:0; top:0; z-index:4999; display:none; opacity:0.6;}
.dialog-box{
	position:fixed; top:-35%; z-index:5000; width:px(226); height:auto; background-color:#fff; border-radius:5px;
	p{padding:px(14); border-bottom:solid 1px #b2b2b2; font-size:px(16); line-height:1.5;}
	.btn{
		padding:px(8) px(10); text-align:center;
		a, span{display:inline-block; width:px(94); padding:px(10) 0; color:#fff; background-color:#ff8a00; border-radius:5px; font-size:px(18);}
		a{margin-right:px(10);}
	}
}

.login-box{
	width:px(320); margin:0 auto; padding-top:px(52); text-align:center;
	.rount-rect{display:inline-block; width:px(176); padding:px(14) 0; margin-top:px(10);
		font-size:px(16); border:solid 1px #ed8b22; border-radius:5px; color:#fff; background-image:linear-gradient(top, #fd9e37, #ff7c00);}
	.tips{margin-top:px(36); p{line-height:1.8;}}
}

//personal center
.ac{width:px(308); margin:0 auto; padding:px(16) px(8);}
.person-inf{
	a{display:-webkit-box; -webkit-box-orient:horizontal; padding:px(6) 0;}
	span,em{display:block;}
	.pic{width:px(56); height:px(50); margin-right:px(16);}
	em{height:px(50); line-height:px(50); font-size:px(15); color:#141414;}
}
.tab-wrap{
	margin-top:px(8);
	nav ul{display:-webkit-box; -webkit-box-orient:horizontal;}
	nav li{
		-webkit-box-flex:1; margin:0 px(4);
		&:first-child{margin-left:0;}
		&:last-child{margin-right:0; em{font-size:px(13); color:#969696;}}
		a{display:table-cell; height:px(66); width:px(96); font-size:px(12); border-color:transparent transparent #d6d6d6;
			text-align:center; vertical-align:middle; line-height:1.6; background:#eaeaea;
			&.selected{background:#ff8400; color:#fff; em{color:#fff;}}
		}
		em{display:block; font-size:px(16); color:#fe8b10;}
	}

	.main{
		h2{font-size:px(16); padding:px(14) px(10); margin-top:px(10);}
		.items-loading{min-height:px(100); background:url("../img/loading.gif") no-repeat center;}
		.desc p{
			font-size:px(15); margin-top:px(10); line-height:1.6; border-bottom:dashed 1px #ddd;
			time{text-align:right; display:block; font-size:px(12); color:#fe9b39; padding:px(10) 0;}
		}
		.no-order{height:px(36); padding-top:px(86); text-align:center;
			background:url("../img/ico_noOrder.png") no-repeat center px(20); background-size:px(36) px(48);}
		.jf-rule{
			h3{font-size:px(16); margin-top:px(14);}
			p{border:0; border-top:dashed 1px #ddd;}
		}
		.btn span{display:block; width:px(182); margin:px(10) auto 0; padding:px(14) 0; text-align:center;}
	}

	.main.rount-rect{
		border-color:#bababa; margin-top:px(12); padding:0 px(8); line-height:1.6; font-size:px(14);
		li{margin-top:px(8); padding-left:px(46);}
		li b{display:inline-block; width:px(46); margin-left:px(-46);}
		.edit{text-align:right;}
	}

	.main.img-text-list{
		li{position:relative; border-bottom:dashed 1px #dadada;}
		.img-text{
			a{display:-webkit-box; -webkit-box-orient:horizontal; padding:px(10) 0 px(12);}
			a + .pay{
				margin:px(-6) 0 px(14); position:relative; font-size:px(14);
				&:after{content:""; width:px(8); height:px(8); position:absolute; top:px(-32); right:px(10);
					border:solid #757575; border-width:2px 2px 0 0; transform:rotate(45deg);}
			}
			span, em{display:block; font-size:px(14); color:#5d5d5d;}
			em:first-child{font-size:px(15);}
			em strong, .pay strong{color:#fe9740; font-weight:normal;}
			.pic{width:px(86); margin-right:px(6); overflow:hidden;}
			.text{line-height:1.6; width:px(204);} b{color:#df1212;}
			s{color:#a0a0a0; margin-top:px(6); font-size:px(14); display:block;}
		}
		.now-buy{
			position:absolute; right:0; bottom:px(12); width:px(80);
			&:after{content:"»"; display:inline; font-size:px(16); padding-left:px(2); color:#ff8b13;}
			a{color:#ff8b13; font-size:px(14);}
		}
	}

	.main.no-list{padding:px(114) 0 px(20); text-align:center;
		background:url("../img/ico_noList.png") no-repeat center px(44); background-size:px(62) px(50);}
}

//personal information
.change-acTaobao{
	margin-top:px(18); display:-webkit-box; -webkit-box-orient:horizontal;
	span{display:block; -webkit-box-flex:1; line-height:1.5; margin-left:px(22);}
	a{
		display:block; width:px(100); margin-right:px(8); text-align:center;
		span{display:block; padding:px(10);}
	}
}
.login-out{text-align:center; margin-top:px(54); a{display:inline-block; padding:px(12) px(60);}}

//order detail
.pay-box .rount-rect{
	border-color:#c9c9c9;
	li{padding:px(12) px(10) px(8); border-bottom:dashed 1px #eee; &:last-child{border-bottom:0;}}
}
.goods-box{
	margin-top:px(10); font-size:px(15);
	.pic{float:left; width:px(100); height:px(100); margin:0 px(10) px(10) px(4);}
	.desc{float:left; width:px(180); line-height:1.5; b{color:#e23131;}}
	.pay{clear:both; text-indent:px(4); s{color:#ff8718;}}
}
.links-btn{
	margin-top:px(16);
	a{display:inline-block; margin-left:px(26); width:px(110); padding:px(14) 0; text-align:center;}
}
.gift-box{
	margin-top:px(16);
	p{padding-left:px(6); font-size:px(14); line-height:1.5;} b{color:#e23131;}
	a{display:block; width:px(182); margin:px(10) auto 0; padding:px(14) 0; text-align:center;}
}

//店铺详情
.shop-detail{
	#iScroll{padding-top:.3rem;}
	.detail-info{margin:0 auto;width:16rem;}
	.shop-info{
		margin:0 .2rem; box-shadow:0 1px 1px #cfd3d4;
	}
	.detail-filed{
		padding:.4rem 0 .4rem .2rem;
		background:#fff;
		dl{display:-webkit-box;}
		.icon{
			width:px(64);height:px(64);
            img{width:100%; height:auto; max-height:px(64);}
		}
		.desc{
			margin-left: .6rem;padding-top:.15rem;
			font-size:px(16);color:#5a5a5a;
		}
		.level{
			margin-top:.2rem;margin-bottom: .6rem;
			img{height:px(14);width:auto;}
		}
		.address{
			color:#b8b8b8;font-size:px(12);
			span{display:inline-block;width:6rem;}
			span:first-child{width:5.6rem;}
		}
	}
	.comment{
		margin-top: .3rem;
		ul{display:-webkit-box;}
		li{
			color:#929292;width:33.3%;
			text-align:center;font-size:px(14);
			border-right:1px solid #e2e2e2;
			margin-top:.2rem;margin-bottom:.3rem;
		}
		li:last-child{border-right:0px;}
		li:after{
			content:'';
		}
	}
	.pic-info{
		margin-top:.5rem;
		margin-left:.2rem;margin-right:.2rem;
		background:#f9fafc;width:px(312);
		a{
			margin:0 .15rem .1rem 0;
			width:5.2rem;
		}
		li{width:34%;}
		li:nth-child(3n+3){
			width:32%;
			a{margin-right:0;}
		}
		.desc{
				position:absolute; right:0; bottom:0; height:.7rem; line-height:.7rem;
				color:#fff; z-index:10;font-size:.5rem;padding-right:.2rem;padding-left:.2rem;
				background:#000; opacity:.4;
			}
	}
}

.item-grid{
	width: px(308);
	margin: px(6) auto 0;
	min-height: px(490);
	ul {
		li {
			position: relative;
			width: px(151); 
			height: px(190);
			border: solid 1px #d2d2d2; 
			box-shadow: 0 2px 3px #ccc; 
			background: white; 
			box-sizing: border-box;
			float: left;
			overflow: hidden;
			margin-top: px(6);
			&:nth-child(2n+1){
				margin-right:px(6);
			}
		}
	}
	.pic {
		a {
			display: block;
			width: px(149);
			height: px(149);
			overflow: hidden;
			text-align: center;
		}
		img {
			width: 100%;
			height: auto;
		}
	}
	.desc {
		display: table;
		width: px(140);
		height: px(45);
		overflow: hidden;
		margin: 0 auto;
		text-align: left;
		a {
			display: table-cell;
			vertical-align: middle;
			padding: px(4) 0;
			color: #333;
			font-size: px(10);
			line-height: px(18);
			word-break: word-all;
		}
	}
}

// 按下的遮罩效果
.J_mask {
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	display: inline-block;
	z-index: 100;
	&.active {
		background-color: rgba(0, 0, 0, 0.05);
	}	
}
// 遮罩效果
.modal {
	display: block;
	position: fixed;
	min-height: px(500);
	top: 0;
	left: 0;
	bottom: 0;
	right: 0;
	z-index: 1000;
	background-color: rgba(0, 0, 0, 0.5);
	&.absolute {
		position: absolute;
	}
	.modal-dialog {
		background:#FFF;
		width: px(226); 
		height: auto; 
		position:absolute; 
		top: 25%; 
		left: 50%; 
		margin-left: px(-113); 
		border-radius(5px);
		p{
			padding:px(14); 
			border-bottom:solid 1px #b2b2b2; 
			font-size:px(16); 
			line-height:1.5;
		}
		.btn{
			padding:px(8) px(10); 
			text-align:center;
			a, span{
				display:inline-block; 
				width:px(94); 
				padding:px(10) 0; 
				color:#fff; 
				background-color:#ff8a00; 
				border-radius:5px; 
				font-size:px(18);}
			a{
				margin-right:px(10);
			}
		}
	}
