/*************** 点状加载提示 start ****************************/
@-webkit-keyframes dotLoadingFrame {
	0% {
		color: transparent;
	}
	100% {
		color: inherit;
	}
}
@keyframes dotLoadingFrame {
	0% {
		color: transparent;
	}
	100% {
		color: inherit;
	}
}
// 点状加载提示
.dot-loading {
	text-align: center;
	min-height: px(100);
	line-height: px(100);
	> span {
		-webkit-animation: dotLoadingFrame 1.5s infinite ease;
		animation: dotLoadingFrame 1.5s infinite ease;
		&:nth-child(1) {
			-webkit-animation-delay: 0.9s;
		    animation-delay: 0.9s;
		}
		&:nth-child(2) {
			-webkit-animation-delay: 1s;
		    animation-delay: 1s;
		}
		&:nth-child(3) {
			-webkit-animation-delay: 1.1s;
		    animation-delay: 1.1s;
		}
		&:nth-child(4) {
			-webkit-animation-delay: 1.2s;
		    animation-delay: 1.2s;
		}
		&:nth-child(5) {
			-webkit-animation-delay: 1.3s;
		    animation-delay: 1.3s;
		}
	}
}
/*************** 点状加载提示 end ****************************/
