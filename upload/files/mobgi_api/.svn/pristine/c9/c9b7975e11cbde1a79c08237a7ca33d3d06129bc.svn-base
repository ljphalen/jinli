/* CSS Document	*/
body {
	font-family: Tahoma, Verdana, Arial;
	font-size: 12px;
	background-color: #cccccc;
	scrollbar-face-color:#FFFFFF;
	scrollbar-highlight-color: #acacac;
	scrollbar-shadow-color:	#acacac;
	scrollbar-3dlight-color: #ffffff;
	scrollbar-darkshadow-color:	#ffffff;
	scrollbar-track-color: #ffffff;
	scrollbar-track-color:#F5F5F5;
	margin:	10px;
	background-attachment: fixed;
	background-image: url(../images/bodybg.png);
	background-repeat: repeat-x;
	background-position: left top;
}
.top {
	background-image: url(../images/topbg.gif);
	background-repeat: repeat-x;
	line-height: 35px;
	width: auto;
	text-align:	center;
	vertical-align:	middle;
}
#nav {
	font-family: Tahoma, MS Shell Dlg;
	font-size: 12px;
	
	PADDING-RIGHT: 0px;	
	PADDING-LEFT: 0px; 
	PADDING-BOTTOM:	0px; 
	MARGIN:	0px	0px	4em; 
	LINE-HEIGHT: 0;	
	PADDING-TOP: 0px;
}
#nav LI	{
	PADDING-RIGHT: 0px;	
	PADDING-LEFT: 0px; 
	BACKGROUND-IMAGE: none;	
	PADDING-BOTTOM:	1px; 
	MARGIN:	0px; 
	LINE-HEIGHT: 1px; 
	PADDING-TOP: 0px; 
	LIST-STYLE-TYPE: none;
}
#nav :link {
	PADDING-RIGHT: 10px; 
	DISPLAY: block;	
	PADDING-LEFT: 0.5em; 
	FONT-WEIGHT: normal; 
	BACKGROUND:	url(./../images/catebg.gif) repeat-x left top;	
	PADDING-BOTTOM:	0px; 
	MARGIN:	0px; 
	COLOR: #000000;	
	LINE-HEIGHT: 1;	
	PADDING-TOP: 0px; 
	TEXT-DECORATION: none;
}
#nav :visited {
	PADDING-RIGHT: 10px; 
	DISPLAY: block;	
	PADDING-LEFT: 0.5em; 
	FONT-WEIGHT: normal; 
	BACKGROUND:	url(./../images/catebg.gif) repeat-x left top;	
	PADDING-BOTTOM:	0px; 
	MARGIN:	0px; 
	COLOR: #000000;	
	LINE-HEIGHT: 1;	
	PADDING-TOP: 0px; 
	TEXT-DECORATION: none;
}
#nav :hover	{
	COLOR: #000000;
	background-color: #efefef;

}
#nav :active {
}
#nav UL	{
	PADDING-RIGHT: 0px;
	PADDING-LEFT: 0px;
	PADDING-BOTTOM:	0px;
	MARGIN:	0px;
	PADDING-TOP: 0px;
	BACKGROUND-COLOR: #FFFFFF;
	border:	0px	none 0;
}
#nav UL	:link {
	PADDING-RIGHT: 10px;
	PADDING-LEFT: 1.5em;
	BACKGROUND-IMAGE: none;
	PADDING-BOTTOM:	4px;
	PADDING-TOP: 4px;
	
	border-left-width: 3px;
	border-left-style: solid;
	border-left-color: #FFFFFF;	
	
}
#nav UL	:visited {
	PADDING-RIGHT: 10px;
	PADDING-LEFT: 1.5em;
	BACKGROUND-IMAGE: none;
	PADDING-BOTTOM:	4px;
	PADDING-TOP: 4px;
	border-left-width: 3px;
	border-left-style: solid;
	border-left-color: #FFFFFF;
}
#nav UL	:hover {
	BORDER-TOP-WIDTH: 1px;
	BORDER-LEFT-WIDTH: 0px;
	BACKGROUND-IMAGE: none;
	BORDER-BOTTOM-WIDTH: 1px;
	BORDER-RIGHT-WIDTH: 0px;
	
	border-left-width: 3px;
	border-left-style: solid;
	border-left-color: #B4BED4;
	border-right-width: 3px;
	border-right-style: solid;
	border-right-color: #B4BED4;
}
td {
	font-family: Tahoma, MS Shell Dlg;
	font-size: 12px;
}
a {
  color: #333333;
  background-color:	transparent;
  text-decoration: none;
}
a:visited {	
  color: #333333;
  background-color:	transparent;
  text-decoration: none;  
}
a:hover	{ 
  color: #CCCCCC;
  text-decoration: underline;
}
#topMenu {
	font-family: Tahoma, MS Shell Dlg;
	font-size: 12px;
	padding: 0;
        
}

#topMenu a {
	font-size: 12px;
	padding: 1px;
	border: 1px solid #FFFFFF;	
	margin: 1px;	
	height: 18px;
	BACKGROUND: #FFFFFF; 
}
#topMenu a:link,#topMenu a:active,#topMenu a:visited {
	color: #000000;
	padding: 1px;
	border: 1px solid #FFFFFF;	
	margin: 1px;
	height: 18px;	
}
#topMenu a:hover {
	BACKGROUND: #B4BED4; 
	padding: 1px;
	border: 1px solid #000000;
	margin: 1px;
	text-decoration: none;
	height: 18px;
	COLOR: #000000;	
}
